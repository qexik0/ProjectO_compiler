// the number of given n in the list 

class Program is
    this(n: Integer, list: List[Integer]) is
        var result: Integer(0)
        var i : Integer(0)
        while i.Less(list.Length()) loop
            if n.Equal(list.get(i)) then
                result := result.Plus(1)
            end
            i := i.Plus(1)
        end
        Console.println(result)
    end
end