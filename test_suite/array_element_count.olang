// the number of given n in the array 

class Program is
    this(n: Integer, array: Array[Integer]) is
        var result: Integer(0)
        var i : Integer(0)
        while i.Less(array.Length) then
            if n.Equal(array.get(i)) then
                result := result.Plus(1)
            end
            i := i.Plus(1)
        end
        Console.println(result)
    end
end